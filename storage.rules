rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /icons/{userId}/{imageId} {
      allow read;
      allow write: if request.auth.uid == userId && userId != 'default' && request.resource.size < 1024 * 1024 && request.resource.contentType.matches('image/.*');
    }
  }
}
